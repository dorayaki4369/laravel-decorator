{
    "name": "dorayaki4369/decoravel",
    "description": "The decorator package for Laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "dorayaki4369",
            "email": "dorayaki4369@outlook.jp"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.41",
        "symfony/finder": "^7.2",
        "composer/class-map-generator": "^1.5",
        "nikic/php-parser": "^5.4"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.45",
        "laravel/pint": "^1.20",
        "orchestra/testbench": "^9.9",
        "phpunit/phpunit": "^11.5",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Dorayaki4369\\Decoravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dorayaki4369\\Decoravel\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
